int correct=0;

int input()
{
	char answer;
	cout << "Your input is: ";
	cin >> answer;
	return (toupper(answer));
}

void Check(bool x)
{
	if(x==1)
	{
		cout << "You are correct!" << endl;
		correct++;
	}
	else if (x==0)
	{
		cout << "You are wrong!" << endl;
	}
}

void testA(char x)
{
	if (x=='A')
	{
		Check(1);
	}
	else if (x!='A')
	{
		Check(0);
	}	
}

void testB(char x)
{
	if (x=='B')
	{
		Check(1);
	}
	else if (x!='B')
	{
		Check(0);
	}	
}

void testC(char x)
{
	if (x=='C')
	{
		Check(1);
	}
	else if (x!='C')
	{
		Check(0);
	}	
}
void testD(char x)
{
	if (x=='D')
	{
		Check(1);
	}
	else if (x!='D')
	{
		Check(0);
	}	
}

void Siao()
{
	char answer;
	cout << "1. What is 3x^2+5x+2 if f(x) is 2?" << endl;
	cout << "a.20\tb.26\nc.24\td.18" << endl;
	answer = input();
	testC(answer);
	cout << endl;
	cout << "2. What is x^3-2x^2+3x+1 if x is 5?" << endl;
	cout << "a.91\tb.161\nc.125\td.100" << endl;
	answer = input();
	testA(answer);
	cout << endl;
	cout << "3. What is f(x) + g(x) if f(2) = 2x^3+3x^2-7x+2 and g(3) = 4x+2?" << endl;
	cout << "a.32\tb.40\nc.28\td.30" << endl;
	answer = input();
	testD(answer);
	cout << endl;
	cout << "4. What is f(g(x)) if f(g(x)) = 5x^2-5x-100, g(x) = 2x^2-9x+5? and x is 5" << endl;
	cout << "a.300\tb.350\nc.325\td.375" << endl;
	answer = input();
	testB(answer);
	cout << endl;
	cout << "5. What is f(x) x ((g(x)+h(x)) if f(x=2) = 2x+2, g(x=3) = 3x^2+5x+2 and h(x=4) = 2x^3-4x^2+x+2?" << endl;
	cout << "a.486\tb.468\nc.684\td.648" << endl;
	answer = input();
	testC(answer);
	cout << "\nYour result is " << correct << " out of 5 questions!" << endl;
	system("pause>nul"); 
	cout << "Thank you for taking the test!" << endl << "Returning to menu..." << endl;
	system("pause>nul"); 
}

class Matrix
{
	int MatA[SIZE][SIZE],MatB[SIZE][SIZE],Prod[SIZE][SIZE],Sum[SIZE][SIZE],Difference[SIZE][SIZE],x,y,i,j;
	public:
	void values();
	void sum();
	void diff();
	void prod();
};
	
void Matrix::values()
{
	cout << "Matrix A and B" << endl;
	cout << "Enter the number of rows: ";
	cin >> x;
	cout << "Enter the number of columns: "; 
	cin >> y;
	cout << "Enter elements of first Matrix\n";
	for (i=0; i<x; i++)
	{
		for ( j=0; j<y; j++)
		{
			cin >> MatA[i][j];
		}
	}
	cout << "Enter elements of second Matrix\n";
	for (i=0; i<x; i++)
	{
		for (j=0; j<y; j++)
		{
			cin >> MatB[i][j];
		}
	}
}

void Matrix::sum()
{
	cout << "Sum of Matrices 1 and 2 is\n";
	for (i=0; i<x; i++)
	{
		for ( j=0; j<y; j++)
		{
			Sum[i][j]=MatA[i][j]+MatB[i][j];
			cout << Sum[i][j] << "\t";
		}
	cout << endl;
	}
}

void Matrix::diff()
{
	cout << "Difference of Matrices 1 and 2 (1-2) is\n";
	for (i=0; i<x; i++)
	{
		for ( j=0; j<y; j++)
		{
			Difference[i][j] = MatA[i][j]-MatB[i][j];
			cout << Difference[i][j] << "\t";
		}
		cout << endl;
		}   
}

void Matrix::prod()
{
	cout << "Product of Matrices 1 and 2 is\n";
	for (i=0; i<x; i++)
	{
		for ( j=0; j<y; j++)
		{
			Prod[i][j]=MatA[i][j]*MatB[i][j];
			cout << Prod[i][j] << "\t";
		}
	cout << endl;
	}
}


void Ming()
{
	int input;
	char cont;	
	Matrix mat;
	system("CLS");
	cout << "---------------------------" << endl;
	cout << "|Welcome to Matrix Solver!|" << endl;
	cout << "---------------------------" << endl;
	mat.values();
	do
	{
		system("CLS");
		cout << "What woud you like to do?\n";
		cout << " 1. Sum of A and B\n" << " 2. Difference of A and B\n 3. Product of A and B" << endl << "Your choice is: ";
		cin >> input;
		switch (input)
		{
			case 1:
			{
				mat.sum();
				break;
			}
			case 2:
			{
				mat.diff();
				break;
			}
			case 3:
			{
				mat.prod();
				break;
			}
			default:
			{
				cout << "Your choice is not within range!" << endl;
			}
		}
		cout << "\nDo you want to pick a different operation? \nType Y for Yes \nType N for No\nYour choice is: ";
		cin >> cont;
	} while (toupper(cont)!= 'N');
	cout << "\n";
	cout << "------------------------------------" << endl;
	cout << "|Thank you for using Matrix solver!|" << endl;
	cout << "------------------------------------" << endl;
	system ("pause");
}

class setboi
{
private:
	int *x;
	int size;
	char * name;
	bool *checker;
public:
	setboi();
	setboi(setboi*);
	void setsetboi();
	int getsetboi(int);
	bool getchecker(int);
	int getsize();
	char getname();
	void displayset();
	void displayset(int a);
	void arrange();
	void displayunion(setboi*);
	void displayintersection(setboi*);
	void displaydifference(setboi*);
	void displayproduct(setboi*);
};
int mystoi(char*);
int mycin(int);
int shortmycin(int);
void mycin(char *);
void gotoxy(int x, int y);
bool menuoftwo(string, string, int);
int menuofsix(string, string, string, string, string, string);
setboi::setboi()
{
	system("cls");
	int input = 0;
	char inputs[10];
	name = new char;
	gotoxy(8, 22);
	cout << "Type in a letter, Enter to confirm";
	gotoxy(8, 6);
	cout << "What is the name of the first set: ";
	mycin(inputs);
	gotoxy(8, 22);
	cout << "Type in a number from 0 to 9, Enter to confirm";
	gotoxy(8, 7);
	cout << "What is the Cardinality of Set " << inputs << " : ";
	input = shortmycin(input);
	size = input - 1;
	checker = new bool[100];
	x = new int[input];
	*name = inputs[0];
}
setboi::setboi(setboi *c)
{
	system("cls");
	gotoxy(8, 5);
	cout << "First Set: ";
	c->displayset();
	int input = 0;
	char inputs[10];
	name = new char;
	bool exit = 1;
	while (exit)
	{
		gotoxy(8, 22);
		cout << "Type in a letter, Enter to confirm";
		gotoxy(8, 6);
		cout << "What is the name of the second set: ";
		mycin(inputs);
		gotoxy(8, 22);
		cout << "Type in a number from 0 to 9, Enter to confirm";
		if (inputs[0] == c->getname())
		{
			gotoxy(8, 7);
			cout << "It is the same name as the previous set, Try again!";
		}
		else
		{
			exit = 0;
			gotoxy(8, 7);
			cout << "													";
		}
	}
	gotoxy(8, 7);
	cout << "What is the Cardinality of Set " << inputs << ": ";
	input = shortmycin(input);
	size = input - 1;
	checker = new bool[100];
	x = new int[input];
	*name = inputs[0];
}
char setboi::getname()
{
	return name[0];
}
void setboi::displayset()
{
	int b = 0;
	cout << "Set " << *name << " { ";
	while (b <= size)
	{
		if (b == size)
		{
			cout << x[b] << " ";
			b++;
		}
		else
		{

			cout << x[b] << ", ";
			b++;
		}
	}
	cout << "}";
}
void setboi::displayset(int a)
{
	int b = 0;
	cout << "Set " << *name << " { ";
	while (b < a)
	{
		if (b == size)
		{
			cout << x[b] << " ";
			b++;
		}
		else
		{

			cout << x[b] << ", ";
			b++;
		}
	}
	cout << "}";
}
int setboi::getsetboi(int a)

{
	return x[a];
}
int setboi::getsize()

{
	return size;
}
void setboi::arrange()
{
	int temp = 0;
	for (int a = 0; a < size; a++)
	{
		for (int b = 0; b < size; b++)
		{
			if (x[b] > x[b + 1])
			{
				temp = x[b];
				x[b] = x[b + 1];
				x[b + 1] = temp;
			}
		}
	}
}
void setboi::setsetboi()
{
	int b = 0;
	int input = 0;
	bool exit = 1;
	for (int a = 0; a <= 99; a++)
	{
		checker[a] = 0;
	}
	while (b <= size)
	{
		exit = 1;
		while (exit)
		{
			gotoxy(8, 10);
			displayset(b);
			gotoxy(8, 22);
			cout << "Type in a number from 0 to 99, Enter to confirm";
			gotoxy(8, 12);
			cout << "Set " << *name << " No. " << b + 1 << ":					 ";
			gotoxy(40, 12);
			cout << "  ";
			gotoxy(40, 12);
			x[b] = mycin(input);
			if (checker[x[b]] == 0)
			{
				checker[x[b]] = 1;
				exit = 0;
				gotoxy(8, 13);
				cout << "											   ";
			}
			else
			{
				gotoxy(8, 13);
				cout << "The element is already in the set, Try again\n";
			}
		}
		b++;
	}
}
bool setboi::getchecker(int a)

{
	return checker[a];
}
void setboi::displayunion(setboi* c)
{
	int b = 0;
	cout << "Set " << *name << " U " << c->getname() << " { ";
	while (b <= 99)
	{
		if (checker[b] || c->getchecker(b))
		{
			cout << b << ", ";
		}
		b++;
	}
	while (b < size)
	{


			cout << x[b] << ", ";
			b++;

	}
	cout << "\b\b }";
}
void setboi::displayintersection(setboi* c)
{
	int b = 0;
	cout << "Set " << *name << " ^ " << c->getname() << " { ";
	while (b <= 99)
	{
		if (checker[b] && c->getchecker(b))
		{
			cout << b << ", ";
		}
		b++;
	}
	while (b < size)
	{


		cout << x[b] << ", ";
		b++;

	}
	cout << "\b\b }";
}
void setboi::displaydifference(setboi * c)
{
	int b = 0;
	cout << "Set " << *name << " - " << c->getname() << " {   ";
	while (b <= size)
	{
		if (checker[x[b]] && c->getchecker(x[b]))
		{
			b++;
		}
		else
		{
			cout << x[b] << ", ";
			b++;
		}
	}
	cout << "\b\b }";
}
void setboi::displayproduct(setboi * c)
{
	int b = 0;
	cout << "Set " << *name << " X " << c->getname() << ":\n {   ";
	while (b <= size)
	{
		if (size == b)
		{
			for (int d = 0; d <= c->getsize(); d++)
			{
				cout << "(" << x[b] << ", " << c->getsetboi(d) << ") ";
				cout << ", ";
			}
			cout << "\b\b }";
			b++;
		}
		else
		{
			for (int d = 0; d <= c->getsize(); d++)
			{
				cout << "(" << x[b] << ", " << c->getsetboi(d) << ") ";
				cout << ", ";
			}
			b++;
		}
	}
}
void Luluquisin()
{
	bool exit2 = 1;
	while (exit2)
	{
		string holdthis;
		string holdthis2;
		setboi a;
		a.setsetboi();
		a.arrange();
		setboi b(&a);
		b.setsetboi();
		b.arrange();
		bool exit = 1;
		system("cls");
		while (exit)
		{
			gotoxy(8, 2);
			cout << "Set Operations";
			gotoxy(8, 3);
			cout << "Current Sets: ";
			gotoxy(10, 4);
			a.displayset();
			gotoxy(10, 5);
			b.displayset();
			gotoxy(8, 7);
			cout << "Choose an Operation: ";
			gotoxy(8, 24);
			cout << "W to go UP, S to go DOWN, Enter to SELECT";
			switch (menuofsix("Union", "Intersection", "Difference", "Cross Product", "Reset Sets", "Exit"))
			{
			case 1:
			{
				system("cls");
				gotoxy(8, 20);
				cout << "																				   ";
				gotoxy(8, 20);
				a.displayunion(&b);
				break; }
			case 2:
			{
				system("cls");
				gotoxy(8, 20);
				cout << "																				   ";
				gotoxy(8, 20);
				a.displayintersection(&b); break; }
			case 3:
			{
				gotoxy(8, 20);
				cout << "																				   ";
				gotoxy(8, 17);
				cout << "Which Operation:";
				holdthis = "Set ";
				holdthis = holdthis + a.getname();
				holdthis = holdthis + " - ";
				holdthis = holdthis + b.getname();
				holdthis2 = "Set ";
				holdthis2 = holdthis2 + b.getname();
				holdthis2 = holdthis2 + " - ";
				holdthis2 = holdthis2 + a.getname();
				if (menuoftwo(holdthis, holdthis2, 9))
				{

					system("cls");
					gotoxy(8, 20);
					a.displaydifference(&b);
				}
				else
				{

					system("cls");
					gotoxy(8, 20);
					b.displaydifference(&a);
				}holdthis = ""; holdthis2 = ""; break; }
			case 4:
			{
				gotoxy(8, 20);
				cout << "																				   ";
				gotoxy(8, 17);
				cout << "Which Operation:";
				holdthis = "Set ";
				holdthis = holdthis + a.getname();
				holdthis = holdthis + " X ";
				holdthis = holdthis + b.getname();
				holdthis2 = "Set ";
				holdthis2 = holdthis2 + b.getname();
				holdthis2 = holdthis2 + " X ";
				holdthis2 = holdthis2 + a.getname();
				if (menuoftwo(holdthis, holdthis2, 9))
				{

					system("cls");
					gotoxy(8, 20);
					a.displayproduct(&b);
				}
				else
				{
					system("cls");
					gotoxy(8, 20);
					b.displayproduct(&a);
				} holdthis = ""; holdthis2 = ""; break; }
			case 5:
			{
				exit = 0;
				break; }
			case 6:
			{
				exit = 0;
				exit2 = 0;
				break;
			}

			}
		}
	}
}

int main()
	{
		HANDLE color=GetStdHandle(STD_OUTPUT_HANDLE);
		do
		{
			char picker;
			introduct();
			cin >> picker;
			switch(toupper(picker))
			{
				case 'A':
				{
					system("CLS");
					cout << "\n------------------------" << endl;
					cout << "| Welcome to SET THEORY! |" << endl;
					cout << "--------------------------" << endl;
					SetConsoleTextAttribute(color, 11);
					printf("SETS");
					SetConsoleTextAttribute(color, 10);
					cout << " are a collection of objects ('elements'')" << endl;
					cout << "A = {a1, a2, ..., an}   'A contains...'" << endl;
					cout << "Order of elements is meaningless" << endl;
					cout << "It does not matter how often the same element is listed." << endl << endl;
					SetConsoleTextAttribute(color, 11);
					printf("CARDINALITY");
					SetConsoleTextAttribute(color, 10);
					cout << " is the number of elements present inside a set." << endl << endl;
					SetConsoleTextAttribute(color, 11);
					printf("UNION");
					SetConsoleTextAttribute(color, 10);
					cout << " that uses the symbol ";
					SetConsoleTextAttribute(color, 11);
					printf("'U'");
					SetConsoleTextAttribute(color, 10);
					cout << " is one of the fundamental operations through which sets can be combined and related to each other." << endl << endl;
					SetConsoleTextAttribute(color, 11);
					printf("Intersection");
					SetConsoleTextAttribute(color, 10);
					cout << " that uses the symbol ";
					SetConsoleTextAttribute(color, 11);
					printf("'n' or '^'");
					SetConsoleTextAttribute(color, 10);
					cout << " is the set that contains all elements of A that also belong to B\nOr equivalently, all elements of B that also belong to A, but no other elements." << endl << endl;
					SetConsoleTextAttribute(color, 11);
					printf("Cartesian Product");
					SetConsoleTextAttribute(color, 10);
					cout << " that uses the symbol ";
					SetConsoleTextAttribute(color, 11);
					printf("'X'");
					SetConsoleTextAttribute(color, 10);
					cout << " is a mathematical operation that returns a product set or simply the product from multiple sets." << endl << endl;
					SetConsoleTextAttribute(color, 11);
					printf("Complement or Set Difference");
					SetConsoleTextAttribute(color, 10);
					cout << " that uses the symbol ";
					SetConsoleTextAttribute(color, 11);
					printf("' - '");
					SetConsoleTextAttribute(color, 10);
					cout << " is a mathematical operation that returns elements that is found on set A and not on set B" << endl << endl;
					cout << "Let us have a sampler after you press any key!" << endl << endl;
					system("Pause");
					Luluquisin();
					cout << "Going back to the menu..." << endl << "\t";
					system("Pause");
					break;
				}
				case 'B':
				{
					system("CLS");
					cout << "\n-----------------------" << endl;
					cout << "| Welcome to SEQUENCES! |" << endl;
					cout << "-------------------------" << endl;
					SetConsoleTextAttribute(color, 11);
					printf("Sequences");
					SetConsoleTextAttribute(color, 10);
					cout << " represent ordered lists of elements." << endl;
					cout << "It is defined as a function from a subset of N to a set S." << endl;
					cout << "It is also a series of number with a common equation to solve the next number" << endl << endl;
					cout << "For Example:" << endl;
					cout << "1, 3, 5, 7, 9, ...\t The next number would be 11 because each number is added by 2" << endl;
					cout << "2, 5, 10, 17, 26...\t The next number would be 37 because the next number is the square of the number then the product is added by 1" << endl <<endl;
					cout << "Let us have a short quiz!"<< endl << endl; 
					system("Pause");
					system("CLS");
					Steve();
					break;
				}
				case 'C':
				{
					system("CLS");
					cout << "\n-----------------------" << endl;
					cout << "| Welcome to FUNCTIONS! |" << endl;
					cout << "-------------------------" << endl << "A ";
					SetConsoleTextAttribute(color, 11);
					printf("Function");
					SetConsoleTextAttribute(color, 10);
					cout << " is an assignment of exactly one element of B to each element of A." << endl << endl;
					cout << "We write it as f(a) = b" << endl;
					cout << "If f:A->B, we say that A is the domain of f and B is the ";
					SetConsoleTextAttribute(color, 11);
					printf("codomain");
					SetConsoleTextAttribute(color, 10);
					cout << " of f.\nIf f(a) = b, we say that b is the image of a and a is the ";
					SetConsoleTextAttribute(color, 11);
					printf("pre-image");
					SetConsoleTextAttribute(color, 10);
					cout << " of b." << endl << endl;
					system("pause>nul");
					cout << "For Example:" << endl;
					system("pause>nul");
					cout << "If f(x) = 2x" << endl << "This will lead to: \nf(1) = 2(1)\n" << "f(3) = 2(3)\n"  << "f(-3) = 2(-3)\n" << endl << endl;
					system("pause>nul");
					cout << "Let us have a short quiz!" << endl;
					system("pause>nul");
					system("CLS");
					Siao();
					break;
				}
				case 'D':
				{
					cout << "\n--------------------------------------" << endl;
					cout << "| Welcome to the GCD and LCM Lesson! |" << endl;
					cout << "--------------------------------------" << endl;
					cout << "Let a and b be integers, not both zero." << endl;
					cout << "The largest integer d such that d | a and d | b is called the ";
					SetConsoleTextAttribute(color, 7);
					printf("Greatest Common Divisor");
					SetConsoleTextAttribute(color, 10);
					cout << " of a and b.\nThe greatest common divisor of a and b is denoted by gcd(a, b)." << endl << endl;
					cout << "For Example:" << endl;
					cout << "What is the GCD of 48 and 72?" << endl;
					system("pause>nul"); 
					cout << "The positive common divisors of 48 and 72 are 1, 2, 3, 4, 6, 8, 12, 16, and 24, so gcd(48, 72) = 24. " << endl << endl;
					cout << "Meanwhile, the ";
					SetConsoleTextAttribute(color, 7);
					printf("Least Common Multiple");
					SetConsoleTextAttribute(color, 10);
					cout << " is the smallest positive integer that is divisible by both a and b." << endl << endl;
					cout << "For Example:" << endl << "What is the Least Common Multiple of 60 and 54?" << endl;
					system("pause>nul"); 
					cout << "Their least common multiple is 540" << endl << "By the process of: " << endl;
					cout << "a = 60 = 2^2 3^1 5^1" << endl << "b = 54 = 2^1 3^3 5^0" << endl;
					cout << "lcm(a, b) = 2^2 3^3 5^1 = 4x27x5 = 540" << endl << endl;
					cout << "Let us have a quiz!" << endl;
					system("Pause");
					system("CLS");
					Cazzey();
					break;
				}
				case 'E':
				{
					system("CLS");
					cout << "\n----------------------" << endl;
					cout << "| Welcome to MATRICES! |" << endl;
					cout << "------------------------" << endl << "A ";
					SetConsoleTextAttribute(color, 7);
					printf("Matrix");
					SetConsoleTextAttribute(color, 10);
					cout << " is a rectangular array of numbers." << endl << "A ";
					SetConsoleTextAttribute(color, 7);
					printf("Matrix");
					SetConsoleTextAttribute(color, 10);
					cout << " with m rows and n columns is called as a ";
					SetConsoleTextAttribute(color, 7);
					printf("mxn Matrix");
					SetConsoleTextAttribute(color, 10);
					cout << endl << endl << "The sum of A and B, denoted by A+B, is the mxn matrix that has a(sub'i',sub'j')+ b(sub'i',sub'j') as its (i, j)th element.";
					system("pause>nul");
					cout << endl << endl <<"Let us use the Matrix Solver!" << endl;
					system("pause>nul");
					Ming();
					break;
				}
				case 'X':
				{
					cout << "Thank you for your time and have a nice day!" << endl;
					break;
				}
				default:
				{
					cout << "Your choice is not within range!" << endl;
					break;
				}
			}

		} while (1);
		system("Pause");
		return 0;
}
int mystoi(char * x)
{
	char * y = x;
	int z = 0;
	int a = 0;
	while (*y != '\0')
	{
		y++;
		z++;
	}
	y = x;
	z--;
	for (int b = 0; z >= 0; b++)
	{
		a = a + ((*(&y[z]) - 48) * (pow(10, b)));
		z--;
	}
	return a;

}
int mycin(int dummy)
{
	int charcounter = 0;
	char x[5];
	int a = 0;
	char *y;
	char *b;
	y = x;
	while (a != 13)
	{
		a = _getch();
		if (a == 8)
		{
			if (y != x)
			{
				b = &x[0];
				while (*b != '\0')
				{
					b++;
				}
				b--;
				*b = '\0';
				cout << "\b \b";
				y--;
				charcounter--;
			}
		}
		else if ((a != 13 && isdigit(a)) && (charcounter < 2))
		{
			*y = (char)a;
			y++;
			*y = '\0';
			cout << (char)a;
			charcounter++;
		}
		else if (a == 13 && charcounter == 0)
		{
			a = 0;
		}
	}
	cout << endl;
	return mystoi(x);
}
int shortmycin(int dummy)
{
	int charcounter = 0;
	char x[5];
	int a = 0;
	char *y;
	char *b;
	y = x;
	while (a != 13)
	{
		a = _getch();
		if (a == 8)
		{
			if (y != x)
			{
				b = &x[0];
				while (*b != '\0')
				{
					b++;
				}
				b--;
				*b = '\0';
				cout << "\b \b";
				y--;
				charcounter--;
			}
		}
		else if ((a != 13 && isdigit(a)) && (charcounter < 1))
		{
			*y = (char)a;
			y++;
			*y = '\0';
			cout << (char)a;
			charcounter++;
		}
		else if (a == 13 && charcounter == 0)
		{
			a = 0;
		}
	}
	cout << endl;
	return mystoi(x);
}
void mycin(char *x)
{
	int charcounter = 0;
	int a = 0;
	char *y;
	char *b;
	y = x;
	while (a != 13)
	{
		a = _getch();
		if (a == 8)
		{
			if (y != x)
			{
				b = &x[0];
				while (*b != '\0')
				{
					b++;
				}
				b--;
				*b = '\0';
				cout << "\b \b";
				y--;
				charcounter--;
			}
		}
		else if ((a != 13 && (charcounter < 1)) && isalpha(a))
		{
			*y = (char)a;
			y++;
			*y = '\0';
			cout << (char)a;
			charcounter++;
		}
		else if (a == 13 && charcounter == 0)
		{
			a = 0;
		}
	}
	cout << endl;
}

int menuofsix(string a, string b, string c, string d, string e, string f)
{
	bool exist = true;
	int arrow = 1;
	int x;
	gotoxy(10, 10);
	cout << a;
	gotoxy(10, 11);
	cout << b;
	gotoxy(10, 12);
	cout << c;
	gotoxy(10, 13);
	cout << d;
	gotoxy(10, 14);
	cout << e;
	gotoxy(10, 15);
	cout << f;
	gotoxy(8, 10);
	cout << "->";
	gotoxy(8, 11);
	cout << "  ";
	gotoxy(8, 12);
	cout << "  ";
	gotoxy(8, 13);
	cout << "  ";
	gotoxy(8, 14);
	cout << "  ";
	gotoxy(8, 15);
	cout << "  ";
	while (exist)
	{
		x = _getch();
		if ((x == 119) || (x == 87))
			arrow--;
		else if ((x == 115) || (x == 83))
			arrow++;
		else if (x == 13)
			exist = false;
		if (arrow == 0)
		{
			arrow = 6;
			gotoxy(8, 10);
			cout << "  ";
			gotoxy(8, 11);
			cout << "  ";
			gotoxy(8, 12);
			cout << "  ";
			gotoxy(8, 13);
			cout << "  ";
			gotoxy(8, 14);
			cout << "  ";
			gotoxy(8, 15);
			cout << "->";
		}
		else if (arrow == 1)
		{
			gotoxy(8, 10);
			cout << "->";
			gotoxy(8, 11);
			cout << "  ";
			gotoxy(8, 12);
			cout << "  ";
			gotoxy(8, 13);
			cout << "  ";
			gotoxy(8, 14);
			cout << "  ";
			gotoxy(8, 15);
			cout << "  ";
		}
		else if (arrow == 2)
		{
			gotoxy(8, 10);
			cout << "  ";
			gotoxy(8, 11);
			cout << "->";
			gotoxy(8, 12);
			cout << "  ";
			gotoxy(8, 13);
			cout << "  ";
			gotoxy(8, 14);
			cout << "  ";
			gotoxy(8, 15);
			cout << "  ";
		}
		else if (arrow == 3)
		{
			gotoxy(8, 10);
			cout << "  ";
			gotoxy(8, 11);
			cout << "  ";
			gotoxy(8, 12);
			cout << "->";
			gotoxy(8, 13);
			cout << "  ";
			gotoxy(8, 14);
			cout << "  ";
			gotoxy(8, 15);
			cout << "  ";
		}
		else if (arrow == 4)
		{
			gotoxy(8, 10);
			cout << "  ";
			gotoxy(8, 11);
			cout << "  ";
			gotoxy(8, 12);
			cout << "  ";
			gotoxy(8, 13);
			cout << "->";
			gotoxy(8, 14);
			cout << "  ";
			gotoxy(8, 15);
			cout << "  ";
		}
		else if (arrow == 5)
		{
			gotoxy(8, 10);
			cout << "  ";
			gotoxy(8, 11);
			cout << "  ";
			gotoxy(8, 12);
			cout << "  ";
			gotoxy(8, 13);
			cout << "  ";
			gotoxy(8, 14);
			cout << "->";
			gotoxy(8, 15);
			cout << "  ";
		}
		else if (arrow == 6)
		{
			gotoxy(8, 10);
			cout << "  ";
			gotoxy(8, 11);
			cout << "  ";
			gotoxy(8, 12);
			cout << "  ";
			gotoxy(8, 13);
			cout << "  ";
			gotoxy(8, 14);
			cout << "  ";
			gotoxy(8, 15);
			cout << "->";
		}
		else if (arrow == 7)
		{
			arrow = 1;
			gotoxy(8, 10);
			cout << "->";
			gotoxy(8, 11);
			cout << "  ";
			gotoxy(8, 12);
			cout << "  ";
			gotoxy(8, 13);
			cout << "  ";
			gotoxy(8, 14);
			cout << "  ";
			gotoxy(8, 15);
			cout << "  ";
		}
	}

	return arrow;
}
bool menuoftwo(string a, string b, int n)
{
	bool exist = true;
	int arrow = 1;
	int x;
	gotoxy(10, 9 + n);
	cout << a;
	gotoxy(10, 10 + n);
	cout << b;
	gotoxy(8, 9 + n);
	cout << "->";
	gotoxy(8, 10 + n);
	cout << "  ";
	while (exist)
	{
		x = _getch();
		if ((x == 119) || (x == 87))
			arrow++;
		else if ((x == 115) || (x == 83))
			arrow--;
		else if (x == 13)
			exist = false;
		if (arrow == -1)
		{
			arrow = 1;
			gotoxy(8, 9 + n);
			cout << "->";
			gotoxy(8, 10 + n);
			cout << "  ";
		}
		else if (arrow == 0)
		{
			gotoxy(8, 9 + n);
			cout << "  ";
			gotoxy(8, 10 + n);
			cout << "->";
		}
		else if (arrow == 1)
		{
			gotoxy(8, 9 + n);
			cout << "->";
			gotoxy(8, 10 + n);
			cout << "  ";
		}
		else if (arrow == 2)
		{
			arrow = 0;
			gotoxy(8, 9 + n);
			cout << "  ";
			gotoxy(8, 10 + n);
			cout << "->";

		}
	}
	return (bool)arrow;
}
void gotoxy(int x, int y)
{
	static HANDLE h = NULL;
	if (!h)h = GetStdHandle(STD_OUTPUT_HANDLE);
	COORD c = { x, y };
	SetConsoleCursorPosition(h, c);
}
	
