{
	int b = 0;
	cout << "Set " << *name << " U " << c->getname() << " { ";
	while (b <= 99)
	{
		if (checker[b] || c->getchecker(b))
		{
			cout << b << ", ";
		}
		b++;
	}
	while (b < size)
	{


			cout << x[b] << ", ";
			b++;

	}
	cout << "\b\b }";
}
void setboi::displayintersection(setboi* c)
{
	int b = 0;
	cout << "Set " << *name << " ^ " << c->getname() << " { ";
	while (b <= 99)
	{
		if (checker[b] && c->getchecker(b))
		{
			cout << b << ", ";
		}
		b++;
	}
	while (b < size)
	{


		cout << x[b] << ", ";
		b++;

	}
	cout << "\b\b }";
}
