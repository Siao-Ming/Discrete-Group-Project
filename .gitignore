char setboi::getname()
{
	return name[0];
}
void setboi::displayset()
{
	int b = 0;
	cout << "Set " << *name << " { ";
	while (b <= size)
	{
		if (b == size)
		{
			cout << x[b] << " ";
			b++;
		}
		else
		{

			cout << x[b] << ", ";
			b++;
		}
	}
	cout << "}";
}
void setboi::displayset(int a)
{
	int b = 0;
	cout << "Set " << *name << " { ";
	while (b < a)
	{
		if (b == size)
		{
			cout << x[b] << " ";
			b++;
		}
		else
		{

			cout << x[b] << ", ";
			b++;
		}
	}
	cout << "}";
}
int setboi::getsetboi(int a)

{
	return x[a];
}
int setboi::getsize()

{
	return size;
}
void setboi::arrange()
{
	int temp = 0;
	for (int a = 0; a < size; a++)
	{
		for (int b = 0; b < size; b++)
		{
			if (x[b] > x[b + 1])
			{
				temp = x[b];
				x[b] = x[b + 1];
				x[b + 1] = temp;
			}
		}
	}
}
void setboi::setsetboi()
{
	int b = 0;
	int input = 0;
	bool exit = 1;
	for (int a = 0; a <= 99; a++)
	{
		checker[a] = 0;
	}
	while (b <= size)
	{
		exit = 1;
		while (exit)
		{
			gotoxy(8, 10);
			displayset(b);
			gotoxy(8, 22);
			cout << "Type in a number from 0 to 9, Enter to confirm";
			gotoxy(8, 12);
			cout << "Set " << *name << " No. " << b + 1 << ":                     ";
			gotoxy(40, 12);
			x[b] = mycin(input);
			if (checker[x[b]] == 0)
			{
				checker[x[b]] = 1;
				exit = 0;
				gotoxy(8, 13);
				cout << "                                               ";
			}
			else
			{
				gotoxy(8, 13);
				cout << "The element is already in the set, Try again\n";
			}
		}
		b++;

	}

}
